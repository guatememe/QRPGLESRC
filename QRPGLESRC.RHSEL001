     C                   CLEAR                   SEL
     C                   CLEAR                   NOMBRE
     C                   WRITE     CTRL001
     C                   EVAL      *IN32='0'
     C                   EXSR      CARGAR
     C                   ENDSR
     ‚**************************************************************************
      **  CARGAR DATOS
     ‚**************************************************************************
     C     CARGAR        BEGSR
     C   15*LOVAL        SETLL     RHEMPL01
     C   16KSITUA        SETLL     RHEMPL02
     C   17KSITUB        SETLL     RHEMPL03
     C   15              READ      RHEMPL01                               40
     C   16              READ      RHEMPL02                               40
     C   17              READ      RHEMPL03                               40
     C                   DOW       *IN40 = '0'
     C***  '601101':6    SCAN      A_CTCOD
     C***                IF        %FOUND
     C***                EXSR      IMPRIMIR
     C***                ENDIF
     C                   MOVEL     NOMAPE        NOMBRE
